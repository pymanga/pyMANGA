<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <random_seed> 10 </random_seed>
    <resources>
        <aboveground>
            <type> Default </type>
        </aboveground>
        <belowground>
            <type> SimpleTest </type>
        </belowground>
    </resources>
    <plant_dynamics>
        <type> SimpleTest </type>
    </plant_dynamics>
    <population>
        <group>
            <name> Recruiting </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 0 </n_individuals>
                <n_recruitment_per_step> 5 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 5 </n_individuals>
                <n_recruitment_per_step> 5 </n_recruitment_per_step>
            </distribution>
        </group>
    </population>
    <time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 86400 </t_end> 
        <delta_t> 43200 </delta_t> <!--Time step length and time span arbitrarily chosen, target: short model runtime-->
    </time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <output>
        <type> OneTreeOneFile </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> Benchmarks/TestOutputs/ </output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <parameter_output> k_geom </parameter_output>
    </output>
</MangaProject>

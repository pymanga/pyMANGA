<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <tree_dynamics>
        <aboveground_competition>
            <type> SimpleTest </type> 
        </aboveground_competition>
        <belowground_competition>
            <type> FixedSalinity_t_var </type>
            <salinity_file> test/SmallTests/inputFiles/salinity.csv </salinity_file>
            <max_x> 5 </max_x>
            <min_x> 0 </min_x>
        </belowground_competition>
        <tree_growth_and_death>
            <type> SimpleBettina </type>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 5 </x_2>
                    <y_2> 5 </y_2>
                </domain>
                <n_individuals> 1 </n_individuals>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 864000 </t_end>
        <delta_t> 86400 </delta_t>
    </tree_time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <tree_output>
        <type> OneTreeOneFile </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> test/testoutputs/ </output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <geometry_output> r_crown </geometry_output>
        <geometry_output> r_root </geometry_output>
        <growth_output> salinity </growth_output>
    </tree_output>
</MangaProject>

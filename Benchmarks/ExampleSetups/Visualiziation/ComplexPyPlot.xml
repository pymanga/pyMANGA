<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <random_seed> 10 </random_seed>
    <resources>
        <aboveground>
            <type> Default </type>
        </aboveground>
        <belowground>
            <type> FixedSalinity </type>
            <min_x>0</min_x>
            <max_x>50</max_x>
            <salinity>0.035 0.08</salinity>
        </belowground>
    </resources>
    <plant_dynamics>
        <type> Bettina </type>
        <mortality> NoGrowth </mortality>
    </plant_dynamics>
    <population>
        <group>
            <name> Avicennia </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 50 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 50 </n_individuals>
                <n_recruitment_per_step> 50 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Rhizophora </name>
            <species> Rhizophora </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 50 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 50 </n_individuals>
                <n_recruitment_per_step> 50 </n_recruitment_per_step>
            </distribution>
        </group>
    </population>
    <time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 10e6 </t_end>
        <delta_t> 1e6 </delta_t>
    </time_loop>
    <visualization>
        <type> ComplexPyplot </type>
        <fps> 1 </fps>
    </visualization>
    <output>
        <type> NONE </type>
    </output>
</MangaProject>
<MangaProject>
    <random_seed>643879</random_seed>
    <resources>
        <aboveground>
            <type> Default </type>
         </aboveground>
        <belowground>
            <type> Default </type>
        </belowground>
    </resources>
    <population>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <vegetation_model_type> Bettina </vegetation_model_type>
            <mortality>NoGrowth Random</mortality> # Random mortality introduced to reduce the number of trees at the
            <probability>0.4</probability>        # end of the simulation and keep the CSV file manageable.
            <domain>
                <x_1> 0 </x_1>
                <y_1> 0 </y_1>
                <x_2> 22 </x_2>
                <y_2> 22 </y_2>
            </domain>
             <initial_population>
                <type>FromFile</type>
                <filename>Benchmarks/ModuleBenchmarks/PlantModules/Bettina/ag_initial_population.csv</filename>
            </initial_population>
            <production>
                <type>SizeDependent</type>
                <formula>0.2 + 0.01 * 200 * x</formula>
                <x_geometry>r_stem</x_geometry>
                <log1>True</log1>
                <x_min>0.05</x_min>
            </production>
            <dispersal>
                <type> Uniform </type>
            </dispersal>
        </group>
    </population>
    <time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 5e8 </t_end>
        <delta_t> 1e6 </delta_t>
    </time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <output>
        <type> OneTimestepOneFile </type>
        <output_times> [2e6, 5e8] </output_times>
        <allow_previous_output>True</allow_previous_output>
        <output_dir>Benchmarks/ModuleBenchmarks/Population/Production/SizeDependent/ReferenceFiles/SizeDependent</output_dir>
        <geometry_output> r_stem </geometry_output>
    </output>
</MangaProject>


<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <tree_dynamics>
        <aboveground_competition>
            <type> SimpleTest </type>
        </aboveground_competition>
        <belowground_competition>
            <type> FixedSalinity </type>
            <salinity> 0.05 </salinity>
        </belowground_competition>
        <tree_growth_and_death>
            <type> SimpleBettina </type>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name> Recruiting </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 0 </n_individuals>
                <n_recruitment_per_step> 0 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Initial </name>
            <species> Avicennia </species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 20 </x_2>
                    <y_2> 20 </y_2>
                </domain>
                <n_individuals> 1 </n_individuals>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 2.5e9 </t_end>
        <delta_t> 1e7 </delta_t>
    </tree_time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <tree_output>
        <type> OneTreeOneFile </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir> ./ProjectLib/ExampleSetups/testoutputs/ </output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <geometry_output> r_crown </geometry_output>
        <geometry_output> r_root </geometry_output>
    </tree_output>
</MangaProject>


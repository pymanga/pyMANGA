<?xml version="1.0" encoding="ISO-8859-1"?>
<MangaProject>
    <random_seed>1</random_seed>
    <tree_dynamics>
        <aboveground_competition>
            <type>SimpleAsymmetricZOI</type>
            <domain>
                <x_1>0</x_1>
                <y_1>0</y_1>
                <x_2>20</x_2>
                <y_2>10</y_2>
                <x_resolution>80</x_resolution>
                <y_resolution>40</y_resolution>
            </domain>
        </aboveground_competition>
        <belowground_competition>
            <type>NetworkOGSLargeScale3D</type>
            <f_radius>0.25</f_radius>
            <ogs_project_folder>test/LargeTests/Test_Setups_large/ogs_example_setup</ogs_project_folder>
            <ogs_project_file>testmodel.prj</ogs_project_file>
            <source_mesh>source_domain.vtu</source_mesh>
            <bulk_mesh>testbulk.vtu</bulk_mesh>
            <delta_t_ogs>86400</delta_t_ogs> <!--24 h wie in testmodel.rpj -->
            <abiotic_drivers>
                <seaward_salinity>0.035</seaward_salinity>
            </abiotic_drivers>
        </belowground_competition>
        <tree_growth_and_death>
            <type>NetworkBettina</type>
            <variant>V0_instant</variant>
            <f_growth>0.25</f_growth>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name>Initial</name>
            <species>Avicennia</species>
            <distribution>
                <type>GroupFromFile</type>
                <filename>test/LargeTests/Test_Setups_large/ogs_example_setup/initial_trees.csv</filename>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type>Simple</type>
        <t_start>0</t_start>
        <t_end> 3e9 </t_end> <!-- ~ 95 a-->
        <delta_t> 3e6</delta_t> <!-- ~ 35 d-->
    </tree_time_loop>
    <visualization>
        <type>NONE</type>
    </visualization>
    <tree_output>
        <type>OneTreeOneFile</type>
        <allow_previous_output>True</allow_previous_output>
        <output_each_nth_timestep>1</output_each_nth_timestep>
        <output_dir>test/LargeTests/Test_Setups_large/ogs_example_setup/tree_output_netw/</output_dir>
        <geometry_output>r_stem</geometry_output>
        <geometry_output>h_stem</geometry_output>
        <geometry_output>r_crown</geometry_output>
        <geometry_output>r_root</geometry_output>
        <growth_output>root_surface_resistance</growth_output>
        <growth_output>xylem_resistance</growth_output>
        <growth_output>growth</growth_output>
        <network_output>rgf</network_output>
        <network_output>partner</network_output>
        <network_output>potential_partner</network_output>
        <network_output>water_available</network_output>
        <network_output>water_absorbed</network_output>
        <network_output>water_exchanged</network_output>
        <network_output>psi_osmo</network_output>
    </tree_output>
</MangaProject>


<MangaProject>
    <tree_dynamics>
        <aboveground_competition>
            <type> SimpleAsymmetricZOI </type>
            <domain>
                <x_1> 0 </x_1>
                <y_1> 0 </y_1>
                <x_2> 185 </x_2>
                <y_2> 10 </y_2>
	        <x_resolution> 720 </x_resolution>
	        <y_resolution> 38 </y_resolution>
            </domain>
        </aboveground_competition>

        <belowground_competition>
            <type> OGSLargeScale3D </type>
            <ogs_project_folder>test/LargeTests/Test_Setups_large/example_box_amp_0_5_kappa_1e-11_slope_0_001</ogs_project_folder>
            <ogs_project_file> testmodel.prj </ogs_project_file>
            <abiotic_drivers>
                <seaward_salinity> 0.035 </seaward_salinity>
            </abiotic_drivers>
            <delta_t_ogs> 1500000 </delta_t_ogs>
            <source_mesh> source_domain.vtu </source_mesh>
            <!--bulk_mesh> testbulk.vtu </bulk_mesh-->
            <!--use_old_ogs_results>True</use_old_ogs_results-->
            <python_script>python_script.py</python_script>
        </belowground_competition>
        <tree_growth_and_death>
            <type> SimpleBettina </type>
        </tree_growth_and_death>
    </tree_dynamics>
    <initial_population>
        <group>
            <name> Avi </name>
            <species>test/LargeTests/Test_Setups_large/example_box_amp_0_5_kappa_1e-11_slope_0_001/Avicennia.py</species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 185 </x_2>
                    <y_2> 10 </y_2>
                </domain>
                <n_individuals> 10 </n_individuals>
                <n_recruitment_per_step> 30 </n_recruitment_per_step>
            </distribution>
        </group>
        <group>
            <name> Rhi </name>
            <species>test/LargeTests/Test_Setups_large/example_box_amp_0_5_kappa_1e-11_slope_0_001/Rhizophora.py</species>
            <distribution>
                <type> Random </type>
                <domain>
                    <x_1> 0 </x_1>
                    <y_1> 0 </y_1>
                    <x_2> 185 </x_2>
                    <y_2> 10 </y_2>
                </domain>
                <n_individuals> 10 </n_individuals>
                <n_recruitment_per_step> 30 </n_recruitment_per_step>
            </distribution>
        </group>
    </initial_population>
    <tree_time_loop>
        <type> Simple </type>
        <t_start> 0 </t_start>
        <t_end> 157788000000 </t_end>
        <delta_t> 15778800 </delta_t>
    </tree_time_loop>
    <visualization>
        <type> NONE </type>
    </visualization>
    <tree_output>
        <type> OneTimestepOneFile </type>
        <output_each_nth_timestep> 1 </output_each_nth_timestep>
        <output_dir>test/LargeTests/Test_Setups_large/example_box_amp_0_5_kappa_1e-11_slope_0_001/TreeOutput/</output_dir>
        <geometry_output> r_stem </geometry_output>
        <geometry_output> h_stem </geometry_output>
        <geometry_output> r_crown </geometry_output>
        <geometry_output> r_root </geometry_output>
        <growth_output> salinity </growth_output>
    </tree_output>
</MangaProject>
